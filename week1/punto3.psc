Algoritmo punto5
	definir num1, num2, num3, div1, div2, div3 como real;
	
	escribir "ingresa los tres numeros";
	leer num1, num2, num3;
	
	div1 = num1 / 3;
	div2 = num2 / 3;
	div3 = num3 / 3;
	
	imprimir num1 " divido en 3 es ", div1;
	imprimir num2 " divido en 3 es ", div2;
	imprimir num3 " divido en 3 es ", div3;
	
FinAlgoritmo
